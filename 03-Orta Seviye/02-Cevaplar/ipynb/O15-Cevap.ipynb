   "source": [
    "def agac(n):\n",
    "    cevap = list()\n",
    "    genislik = 2*n-1\n",
    "    for i in range(1,n+1):\n",
    "        kare = \"#\"*(i*2-1)\n",
    "        cevap.append(kare.center(genislik))\n",
    "    return cevap"
   ]
